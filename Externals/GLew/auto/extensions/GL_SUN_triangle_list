	GL_REPLACEMENT_CODE_ARRAY_STRIDE_SUN 0x85C2
	GL_REPLACEMENT_CODE_ARRAY_POINTER_SUN 0x85C3
	GL_R1UI_V3F_SUN 0x85C4
	GL_R1UI_C4UB_V3F_SUN 0x85C5
	GL_R1UI_C3F_V3F_SUN 0x85C6
	GL_R1UI_N3F_V3F_SUN 0x85C7
