#: ../src/xz/xz.1
msgid "Binary Tree with 2-byte hashing"
msgstr "Binärträd med 2-bytehashning"

#. type: Plain text
#: ../src/xz/xz.1
msgid "Minimum value for I<nice>: 2"
msgstr "Minsta värde på I<nice>: 2"

#. type: Plain text
#: ../src/xz/xz.1
msgid "Memory usage: I<dict> * 9.5"
msgstr "Minnesanvändning: I<dict> · 9,5"

